{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug GUI",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "module": "svg2ico",
      "args": ["--preset", "icns"],
      "console": "integratedTerminal",
      "autoReload": {
        "enable": false,
        "include": [
          "**/*.py",
          "**/*.pyw"
        ] 
      },
      "justMyCode": true
    },
    {
      "name": "debug CLI",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "module": "svg2ico",
      "args": ["--preset", "icns", "-i", "example.drawio.svg"],
      "console": "integratedTerminal",
      "autoReload": {
        "enable": false,
        "include": [
          "**/*.py",
          "**/*.pyw"
        ] 
      },
      "justMyCode": true
    }
  ]
}
